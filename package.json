{
  "name": "jsxpath",
  "version": "1.1.0",
  "description": "XPath like notation to query a JSON object",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage && open -a \"Safari\" ./coverage/lcov-report/index.html",
    "build": "tsc"
  },
  "keywords": [
    "jsxpath",
    "json",
    "xml",
    "xpath",
    "js",
    "query",
    "evaluate",
    "language",
    "functions",
    "variables",
    "object"
  ],
  "author": "Quang Nhan <qn.cody@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/Quang-Nhan/JSXPath",
  "repository": {
    "type": "git",
    "url": "https://github.com/Quang-Nhan/JSXPath"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "roots": [
      "<rootDir>/ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "dependencies": {
    "@types/jest": "^29.2.6",
    "jest": "^29.4.0",
    "jest-environment-jsdom": "^29.4.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  }
}
