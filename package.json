{
  "name": "jsxpath",
  "version": "1.0.0-0.0.0",
  "description": "An Adaptation of XPath notation to query a JSON document.",
  "main": "index.js",
  "scripts": {
    "test": "grunt test",
    "jest": "jest",
    "build": "tsc"
  },
  "keywords": [
    "[\"jsxpath\"",
    "\"JSXPath\"",
    "\"json\"",
    "\"xpath\"",
    "\"js\"",
    "\"query",
    "language\"]"
  ],
  "author": "Quang Nhan <qn.cody@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.1.5",
    "ts-jest": "^23.0.0",
    "typescript": "^3.1.3"
  },
  "homepage": "https://github.com/Quang-Nhan/JSXPath",
  "repository": {
    "type": "git",
    "url": "https://github.com/Quang-Nhan/JSXPath"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "roots": [
      "<rootDir>/ts",
      "<rootDir>/test"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "jest": "^23.6.0"
  }
}
